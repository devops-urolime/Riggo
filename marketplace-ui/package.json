{
  "name": "marketplace-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.1.3",
    "@material-ui/icons": "^4.2.1",
    "chai": "^4.2.0",
    "clsx": "^1.0.4",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "history": "^4.9.0",
    "node-sass": "^4.12.0",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.2",
    "query-string": "^6.8.1",
    "react": "^16.8.6",
    "react-device-detect": "^1.6.1",
    "react-dom": "^16.8.6",
    "react-redux": "^5.0.7",
    "react-router": "^5.0.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "3.0.1",
    "react-test-renderer": "^16.8.6",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.0.1",
    "reselect": "^4.0.0",
    "reset-css": "^4.0.1",
    "sinon": "^7.3.2",
    "auth0-js": "^9.11.2",
    "material-components-web": "^3.0.0",
    "@nivo/sankey": "^0.59.1",
    "@nivo/tooltip": "^0.59.1",
    "@nivo/pie": "^0.59.1",
    "@nivo/bar": "^0.59.2",
    "recharts": "^1.6.2"
  },
  "scripts": {
    "env": "export $(cat .env | xargs) && env",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "aws-s3-push-qa": "aws s3 sync ./build s3://$AWS_S3_BUCKET_DEPLOY/ --region $AWS_S3_REGION_DEPLOY --delete",
    "aws-cloudfront-invalidation": "aws cloudfront create-invalidation --distribution-id $AWS_CLOUDFRONT_DISTRIBUTION_ID --paths \"/*\"",
    "build": "npm-run-all build-css build-js",
    "deploy": "npm-run-all aws-s3-push-qa aws-cloudfront-invalidation",
    "build-deploy": "npm-run-all build deploy",
    "test-dev": "react-scripts test --env=jsdom",
    "test": "react-scripts test --watchAll=false --env=jsdom --forceExit",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
